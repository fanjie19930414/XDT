<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- HeaderBar自定义属性 -->
    <declare-styleable name="HeaderBar">
        <attr name="isShowBack" format="boolean"/>
        <attr name="titleText" format="string"/>
        <attr name="rightText" format="string"/>
    </declare-styleable>
    <!-- LabelText自定义属性 -->
    <declare-styleable name="LabelTextView">
        <attr name="labelText" format="string"/>
        <attr name="contentText" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomRoundAngleImageView">
        <attr name="radius" format="dimension"/>
    </declare-styleable>
    <!--  MoreTextView属性  -->
    <declare-styleable name="MoreTextView">
        <attr name="minLine" format="integer"/>
        <attr name="moreTextColor" format="color"/>
        <attr name="moreSwitchTextColor" format="color"/>
        <attr name="moreTextSize" format="dimension"/>
        <attr name="moreSwitchTextSize" format="dimension"/>
        <attr name="moreSwitchDrawable" format="reference"/>
    </declare-styleable>
    <!--  RoundProgressBar属性  -->
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>
</resources>